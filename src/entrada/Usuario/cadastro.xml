<?xml version="1.0" encoding="UTF-8"?>

<processo codigo="cadastro" modulo="Usuario">
    
    <entrada>
        <campo regra="Obrigatorio" chave="Usuario::nome" nome="nome"/> 
        <campo regra="Obrigatorio" chave="Usuario::apelido" nome="apelido"/>
        <campo regra="Obrigatorio" chave="Usuario::telefone" nome="telefone"/>
        <campo regra="Obrigatorio;Data" chave="Usuario::nascimento" nome="nascimento"/>
        <campo regra="Obrigatorio" chave="Usuario::avataresId" nome="avataresId"/>
        <campo regra="Obrigatorio" chave="Usuario::generoId" nome="generoId"/>
        <campo chave="Arquivo" nome="arquivo" type="file"/>
    </entrada>
    
    <atividades>
        <atividade codigo="cadastro-usuario" tipo="tarefa">
            <entidade nome="CadastroUsuario" metodo="cadastro"/>
        </atividade>
    </atividades>
    
    <saida> </saida>
    
</processo>
